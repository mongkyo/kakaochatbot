server {
	return 404;
}
server {
	listen 80;
	listen 443;

	server_name monglab.com www.monglab.com api.monglab.com *.elasticbeanstalk.com;
	charset utf-8;
	client_max_body_size 128M;

	location / {
		uwsgi_pass unix:///tmp/app.sock;
		include    uwsgi_params;
	}
	
	location /media/ {
		alias /srv/project/.media/;
	}

}
